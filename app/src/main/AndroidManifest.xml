<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ht501.luckyyou">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon23"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon23"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Play"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".SinglePlayer" />
        <activity android:name=".MultiPlayer" android:parentActivityName=".Play"/>
    </application>

</manifest>